{
  "name": "직딩 토끼의 하루",
  "action": {
    "default_popup": "index.html"
  },
  "manifest_version": 3,
  "version": "1.0.1",
  "web_accessible_resources": [{
    "matches": [
      "<all_urls>"
    ],
    "resources": [
      "assets/*.css",
      "font/DNFBitBitOTF.otf",
      "font/DNFBitBitTTF.ttf",
      "icons/*.png",
      "images/*.png",
      "border-menu/*.png",
      "sound.webm"
    ]
  }],
  "icons": {
    "16": "icons/carrot128.png",
    "48": "icons/carrot128.png",
    "128": "icons/carrot128.png"
  }
}