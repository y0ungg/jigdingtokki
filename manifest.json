{
  "name": "직딩 토끼의 하루",
  "action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 3,
  "version": "1.0.1",
  "web_accessible_resources": [{
    "matches": [
      "<all_urls>"
    ],
    "resources": [
      "css/font.css",
      "css/popup.css",
      "font/DNFBitBitOTF.otf",
      "font/DNFBitBitTTF.ttf",
      "assets/*.png",
      "images/*.png"
    ]
  }],
  "icons": {
    "16": "assets/carrot.png",
    "48": "assets/carrot.png",
    "128": "assets/carrot.png"
  }
}